model User {
  id    Int    @id @default(autoincrement())
  name  String?
  email String @unique
  password String
  token String? @unique
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:arian@localhost:5432/salud-en-movimiento?schema=public"
}
